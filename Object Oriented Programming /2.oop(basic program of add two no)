#include <iostream>
using namespace std;
class numbers{
    public:
    int a;
    int b;
    int sum;
    void display(){
        cout<<"Sum of two numbers is:"<<sum;
    }
};
int main() {
    numbers n1;
    n1.a,n1.b;
    cout<<"Enter two numbers:";
    cin>>n1.a>>n1.b;
    n1.sum=n1.a+n1.b;
    n1.display();
    return 0;
}
