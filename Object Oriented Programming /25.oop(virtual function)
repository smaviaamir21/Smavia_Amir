#include <iostream>
using namespace std;

class Animal {
public:
    virtual void sound() {
        cout << "animal makes sound" << endl;
    }
};

class Dog : public Animal {
public:
    void sound() override {
        cout << "Dog barks." << endl;
    }
};

int main() {
    Animal* a = new Dog();
    a->sound();  
}
